{
  "name": "forex-rate",
  "version": "1.0.0",
  "description": "FX rates widget",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev && webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "test": "./node_modules/.bin/jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gvnsaxena/forex-rate.git"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-thunk": "^2.1.0"
  },
  "author": "gvnsaxena@gmail",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gvnsaxena/forex-rate/issues"
  },
  "contributors": [
    {
      "name": "Gaurav Saxena",
      "email": "gvnsaxena@gmail.com"
    }
  ],
  "homepage": "https://github.com/gvnsaxena/forex-rate#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-modules-literals": "0.0.3",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "es5-shim": "^4.5.9",
    "html-webpack-plugin": "3.2.0",
    "jest": "21.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^7.0.0",
    "sass-loader": "7.1.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.0"
  },
  "jest": {
    "testMatch": [
      "**/src/**/*spec.js?(x)"
    ],
    "collectCoverage": false,
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 65,
        "lines": 65,
        "statements": 65
      }
    },
    "coverageReporters": [
      "json",
      "html"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.config.js"
  }
}
