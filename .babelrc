    "presets": ["es2015", "react", "stage-0"],
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread",
      ["transform-runtime", {
        "helpers": false,
        "polyfill": true,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }]
    ]
  }
  